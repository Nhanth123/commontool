<?xml version="1.0" encoding="utf-8"?>
<configuration>

    <!-- Keep standard NuGet feeds -->
    <packageSources>
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    </packageSources>

    <!-- Store all restored packages inside the project -->
    <config>
        <add key="globalPackagesFolder" value="./packages" />
    </config>
</configuration>